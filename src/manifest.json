{
  "manifest_version": 3,
  "name": "WebMe",
  "version": "0.1.0",
  "description": "Make the internet yours. A Chrome extension to customize and style any website exactly how you see it.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://youtube.com/*"
      ],
      "js": [
        "src/content.ts"
      ],
      "run_at": "document_idle"
    }
  ]
}